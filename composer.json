{
  "name": "wodby/wodby-sdk-php",
  "description": "Wodby SDK for PHP",
  "homepage": "https://wodby.com",
  "keywords": [
    "wodby",
    "sdk",
    "api",
    "cloud"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "guzzlehttp/guzzle": "^6",
    "php": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Wodby\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
